<header role="banner" class="<%= prefix %>-header">
    <div class="<%= prefix %>-container%>" >
        <%- /* include('./head', locals.head); */ %>

        <div class="<%= prefix %>-header__body">

            <%- include('./brand', locals.brand); %>

            <% if (locals.operator !== undefined) { %><%- include('./operator', operator); %><% } %>

            <%- include('./navbar', navbar); %>

            <% if (locals.tools !== undefined) { %><%- include('./tools', tools); %><% } %>
        </div>

        <%
        switch(typeof locals.navigation) {
        case 'string': %>
        <%- include(navigation, {id: 'header-navigation'}); %>
        <% break;

        case 'object': %>
        <%- include( '../../../navigation/tests/templates/navigation', { ... navigation, id: navigation.id || 'header-navigation'}); %>
        <% break;
        }
        %>

    </div>
</header>